<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body class="p-5">
    <% if (currentUser && breadcrumbs && breadcrumbs.length) { %>
    <wa-breadcrumb class="p-5">
      <% breadcrumbs.forEach(breadcrumb => { %>
      <wa-breadcrumb-item
        ><a href="/folders/<%= breadcrumb.id %>"
          ><%= breadcrumb.folderName %></a
        ></wa-breadcrumb-item
      >
      <% }); %>
    </wa-breadcrumb>
    <%} %>
  </body>
  <script
    type="module"
    src="https://early.webawesome.com/webawesome@3.0.0-beta.4/dist/components/breadcrumb/breadcrumb.js"
  ></script>
</html>
