<body>
  <div
    class="flex justify-between items-center h-[100px] border-solid border-dark-aqua border-b-[1px]"
  >
    <h2 class="p-4 m-2 text-5xl font-semibold"><a href="/">File Uploader</a></h2>
    <div>
      <% if (!currentUser) {%>
      <a href="/login">Log In</a>
      <a href="/sign-up">Sign Up</a>
      <% } else { %>
      <div class="p-5 mx-5 flex flex-row items-center cursor-pointer">
        <svg viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg" class="svg-icon">
          <path
            d="M1.3 3.75h5.88V2.5H1.3A1.25 1.25 0 0 0 .05 3.75v8.5A1.25 1.25 0 0 0 1.3 13.5h5.88v-1.25H1.3z"
          />
          <path
            d="m15.4 7-4-2.74-.71 1 3.08 2.1H4.71v1.26h9.07l-3.08 2.11.71 1L15.4 9a1.24 1.24 0 0 0 0-2z"
          />
        </svg>
        <a href="/logout" class="pl-2.5 text-lg"> Log Out</a>
      </div>
      <% } %>
    </div>
  </div>
</body>
