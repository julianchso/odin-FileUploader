{
  "name": "odin-fileuploader",
  "version": "1.0.0",
  "description": "",
  "main": "server/dist/app.js",
  "types": "src/app",
  "type": "module",
  "scripts": {
    "watch:ts": "cd ./server && tsc",
    "copy:views": "copyfiles -u 3 server/src/views/**/* server/build/views",
    "dev:server": "nodemon server/build/app.js",
    "dev:client": "vite",
    "dev": "concurrently \"npm run watch:ts\" \"npm run copy:views\" \"npm run dev:server\" \"npm run dev:client\"",
    "build:ts": "cd ./server && tsc && npm run copy:views",
    "build:client": "vite build",
    "build": "npm run build:ts && npm run build:client",
    "preview": "vite preview"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.13",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^2.0.0",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "method-override": "^3.0.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.0.15",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "prisma": "^6.12.0",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
